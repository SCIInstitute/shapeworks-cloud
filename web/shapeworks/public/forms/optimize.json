{
    "id": "optimize",
    "type": "object",
    "description": "TODO: Optimize description",
    "properties": {
        "section_1": {
            "title": "Particle System Parameters",
            "type": "object",
            "properties": {
                "number_of_particles": {
                    "type": "integer",
                    "default": 128,
                    "title": "Number of particles"
                },
                "relative_weighting": {
                    "type": "number",
                    "default": 0.05,
                    "title": "Relative weighting",
                    "x-cols": 6
                },
                "initial_relative_weighting": {
                    "type": "number",
                    "default": 0.05,
                    "title": "Initial",
                    "x-cols": 6
                },
                "starting_regularization": {
                    "type": "integer",
                    "default": 1000,
                    "title": "Starting regularization",
                    "x-cols": 6
                },
                "ending_regularization": {
                    "type": "integer",
                    "default": 10,
                    "title": "Ending regularization",
                    "x-cols": 6
                },
                "iterations_per_split": {
                    "type": "integer",
                    "default": 1000,
                    "title": "Iterations per split"
                },
                "optimization_iterations": {
                    "type": "integer",
                    "default": 1000,
                    "title": "Optimization iterations"
                },
                "use_landmarks": {
                    "type": "boolean",
                    "default": false,
                    "title": "Landmarks"
                },
                "use_geodesic_distance": {
                    "type": "boolean",
                    "default": false,
                    "title": "Geodesic distance",
                    "x-cols": 6
                },
                "geodesic_cache_multiplier": {
                    "type": "integer",
                    "default": 20,
                    "title": "Cache multiplier",
                    "x-cols": 6,
                    "x-display": "custom-conditional",
                    "x-display-if": "(model) => model.section_1.use_geodesic_distance"
                },
                "use_normals": {
                    "type": "boolean",
                    "default": false,
                    "title": "Normals",
                    "x-cols": 6
                },
                "normals_strength": {
                    "type": "integer",
                    "default": 10,
                    "title": "Strength",
                    "x-cols": 6,
                    "x-display": "custom-conditional",
                    "x-display-if": "(model) => model.section_1.use_normals"
                },
                "multiscale": {
                    "type": "boolean",
                    "default": false,
                    "title": "Multiscale",
                    "x-cols": 6
                },
                "multiscale_particles": {
                    "type": "integer",
                    "default": 32,
                    "title": "Particles",
                    "x-cols": 6,
                    "x-display": "custom-conditional",
                    "x-display-if": "(model) => model.section_1.multiscale"
                },
                "procrustes": {
                    "type": "boolean",
                    "default": false,
                    "title": "Procrustes",
                    "x-cols": 3
                },
                "procrustes_scaling": {
                    "type": "boolean",
                    "default": false,
                    "title": "Scaling",
                    "x-cols": 2,
                    "x-display": "custom-conditional",
                    "x-display-if": "(model) => model.section_1.procrustes"
                },
                "procrustes_interval": {
                    "type": "integer",
                    "default": 10,
                    "title": "Interval",
                    "x-cols": 3,
                    "x-display": "custom-conditional",
                    "x-display-if": "(model) => model.section_1.procrustes"
                },
                "procrustes_rotation_translation": {
                    "type": "boolean",
                    "default": true,
                    "title": "Rotation/Translation",
                    "x-cols": 4,
                    "x-display": "custom-conditional",
                    "x-display-if": "(model) => model.section_1.procrustes"
                },
                "narrow_band": {
                    "type": "integer",
                    "default": 4,
                    "title": "Narrow band"
                },
                "optimize_output_prefix": {
                    "type": "string",
                    "default": "particles",
                    "title": "Output prefix",
                    "x-display": "custom-conditional",
                    "x-display-if": "(model) => false"
                }
            }
        }
    }
}
